class Solution{
    static int map(int n, String keys[], int arr[], String s)
    {
        HashMap<String,Integer> h=new HashMap<String,Integer>();
        for(int i=0;i<n;i++)
        h.put(keys[i],arr[i]);
            if(h.containsKey(s))
            return h.get(s);
            else
            return -1;
    }
}
